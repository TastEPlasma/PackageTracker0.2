<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Delivered" Color="#FF554355"/>
    <SolidColorBrush x:Key="NotFound" Color="#FFB66363"/>
    <SolidColorBrush x:Key="NotShipped" Color="#FF937D54"/>
    <SolidColorBrush x:Key="Other" Color="#FF4D3C3C"/>
    <SolidColorBrush x:Key="OutForDelivery" Color="#FF4B7C4B"/>
    <SolidColorBrush x:Key="Returned" Color="Gray"/>
    <SolidColorBrush x:Key="Shipped" Color="#FFE0E095"/>
    <SolidColorBrush x:Key="PickUp" Color="#FF325AA2"/>

</ResourceDictionary>